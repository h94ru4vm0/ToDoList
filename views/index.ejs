<html>
	<head>
		<link href="assets/style.css" type="text/css" rel="stylesheet" />
		<link href="assets/bootstrap.min.css" type="text/css" rel="stylesheet" />
		<link href="assets/js/main.js" type="text/css" rel="stylesheet" />
		<link href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
		<link href='https://fonts.googleapis.com/css?family=Merienda+One' rel='stylesheet' type='text/css'>
		<script src="assets/js/jquery.easing.min.js"></script>
		<!-- <script src="https://code.jquery.com/jquery-1.11.3.min.js" ></script> -->
		<script  src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
	</head>
	<body>
		<nav class="navbar navbar-inverse navbar-fixed-top">
	    <!-- <div class="container"> -->
	      <div class="row">
	          <ul class="nav navbar-nav col-sm-12 col-md-12 col-lg-12">
	            <li class="col-sm-3 col-md-3 col-lg-3"><a href="#submit" class="page-scroll navbarli">SUBMIT</a></li>
	            <li class="col-sm-3 col-md-3 col-lg-3"><a href="#data" class="page-scroll navbarli">DATA</a></li>
	            <li class="col-sm-3 col-md-3 col-lg-3"><a href="#" class="navbarli">PRODUCT</a></li>
	            <li class="col-sm-3 col-md-3 col-lg-3"><a href="#" class="navbarli">CONTACT</a></li>
	          </ul>
	      </div>
	    <!-- </div> -->
	  </nav>
		<section class="S1" id="submit">
			<div class="container">
				<div class="row">
					<h1 class="center space">
						<p>ToDoList<span>|</span></p>
					</h1>
					<form method="POST" action="/person" class="text-center">
						title: <br> <input type="text" id="title" name="title"/><br /><br>
						content: <br> <input type="text" id="contain" name="contain" /><br /><br>
						<input type="submit" value="Submit" />
					</form>
				</div>
			</div>
		</section>

		<section id="data">
			<div class="container">
				<div class="row text-center">
					<br><br><br><br><h1>Data Stored</h1><br><br>
				</div>
				<div class="row text-center">
					<% if (userinfos.length) { %>
						<ol>
							<% userinfos.forEach(function(userinfo){ %>
								<li class="data-li">
									<div class="title">
										<%= userinfo['title'] %>
									</div>
									<div class="content">
										<%= userinfo['contain'] %>
									</div>


								</li>

							<% }) %>
						</ol>

						<br>
					<% } %>
				</div>
			</div>
		</section>

		<footer class="navbar-inverse">
      <div class="container">
        <div class="row">
          <h6 class="text-center footer-text">Copyright &copy; 2016</h6>
        </div>
      </div>
    </footer>
	</body>
</html>
